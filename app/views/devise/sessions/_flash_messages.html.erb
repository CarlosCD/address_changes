<% flash.each do |key, value| %>
  <% if key == 'alert' %>
    <div class="alert alert-danger alert-dismissible" role="alert">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <strong><%= value %></strong>
    </div>
  <% end %>
<% end %>
